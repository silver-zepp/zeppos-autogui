{
  "configVersion": "v3",
  "app": {
    "appId": 70001,
    "appName": "ZeppOS + NJS Server + WS",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "Sample project that shows the use of express.js server and websockets"
  },
  "permissions": [ 
    "data:os.device.info", 
    "data:user.hd.heart_rate", 
    "data:user.hd.step", 
    "device:os.accelerometer" 
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "1.0",
      "target": "1.0",
      "minVersion": "3.0"
    }
  },
  "debug": false,
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/common/index"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "st": "r"
        }
      ],
      "designWidth": 480
    }
  },
  "i18n": {
    "en-US": {
      "appName": "ZeppOS + NJS Server + WS"
    }
  },
  "defaultLanguage": "en-US"
}
